  | Supervisor => 1
  | Machine => 2

def privLevel_bits_forwards (arg_ : Privilege) : (BitVec 2) :=
  match arg_ with
  | User => (0b00 : (BitVec 2))
  | Supervisor => (0b01 : (BitVec 2))
  | Machine => (0b11 : (BitVec 2))

def privLevel_bits_backwards (arg_ : (BitVec 2)) : SailM Privilege := do
  let b__0 := arg_
  bif (b__0 == (0b00 : (BitVec 2)))
